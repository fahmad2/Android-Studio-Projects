<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:text="@string/defaultLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/officiallLocation"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:textAppearance="@style/TextAppearance.AppCompat.Button"
        android:layout_marginTop="3dp"
        app:layout_constraintHorizontal_bias="0.501" />

    <TextView
        android:text="OFFICE:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/photoOffice"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toBottomOf="@+id/officiallLocation"
        android:textAppearance="@style/TextAppearance.AppCompat.Button"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.501" />

    <TextView
        android:text="No Name"
        android:layout_width="197dp"
        android:layout_height="30dp"
        android:id="@+id/photoName"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintHorizontal_bias="0.132"
        android:layout_marginTop="-9dp"
        app:layout_constraintTop_toBottomOf="@+id/photoOffice" />

    <ImageView
        android:id="@+id/officialImage"
        android:layout_width="140dp"
        android:layout_height="145dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="3dp"
        android:clickable="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/officialParty"
        app:srcCompat="@drawable/missing"
        app:layout_constraintHorizontal_bias="0.94" />

    <TextView
        android:text="Address:"
        android:layout_width="60dp"
        android:layout_height="23dp"
        android:id="@+id/textView7"
        android:layout_marginStart="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="32dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView4" />

    <TextView
        android:text="Phone:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintLeft_toLeftOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView10"
        tools:layout_editor_absoluteY="288dp"
        tools:layout_editor_absoluteX="32dp" />

    <TextView
        android:text="xxx-xxx-xxxx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:layout_marginLeft="2dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView4"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:text="Email:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        app:layout_constraintLeft_toLeftOf="@+id/textView9"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView13" />

    <TextView
        android:text="No Data Provided"
        android:layout_width="211dp"
        android:layout_height="20dp"
        android:id="@+id/textView13"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintLeft_toLeftOf="@+id/textView10"
        android:layout_marginTop="6dp"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        android:layout_marginLeft="0dp" />

    <TextView
        android:text="Website:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintLeft_toLeftOf="@+id/textView11"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView15" />

    <TextView
        android:text="No Data Provided"
        android:layout_width="211dp"
        android:layout_height="18dp"
        android:id="@+id/textView15"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintLeft_toLeftOf="@+id/textView13"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        android:layout_marginLeft="0dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="239dp"
        android:layout_height="60dp"
        android:text="No Data Provided"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@+id/photoName"
        app:layout_constraintLeft_toRightOf="@+id/textView7"
        android:layout_marginLeft="23dp" />

    <TextView
        android:id="@+id/officialParty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Party"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.845"
        app:layout_constraintBaseline_toBaselineOf="@+id/photoName" />

    <ImageView
        android:id="@+id/facebook"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/facebook"
        android:layout_marginRight="80dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="4dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/officialImage"
        app:layout_constraintVertical_bias="0.325" />

    <ImageView
        android:id="@+id/twitter"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/twitter"
        app:layout_constraintBottom_toBottomOf="@+id/facebook"
        android:layout_marginBottom="0dp"
        app:layout_constraintRight_toLeftOf="@+id/facebook"
        android:layout_marginRight="79dp" />

    <ImageView
        android:id="@+id/googleplus"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/googleplus"
        app:layout_constraintRight_toLeftOf="@+id/twitter"
        android:layout_marginRight="107dp"
        app:layout_constraintBottom_toBottomOf="@+id/twitter"
        android:layout_marginBottom="0dp" />

    <ImageView
        android:id="@+id/youtube"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginBottom="-12dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/googleplus"
        app:layout_constraintHorizontal_bias="0.673"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/googleplus"
        app:srcCompat="@drawable/youtube" />

</android.support.constraint.ConstraintLayout>